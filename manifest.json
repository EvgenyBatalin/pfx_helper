{
  "name": "PrivateFX Helper by pfx.batal.ru",
  "version": "1.5",
  "description": "PrivateFX Helper by pfx.batal.ru",
  "manifest_version": 2,
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": [ "https://my.privatefx.com/*" ],
      "js": [ "jquery-2.1.3.min.js", "accountsMoney.js" ]
    },
    {
      "matches": [ "https://my.privatefx.com/investor*"],
      "js": [ "jquery-2.1.3.min.js","datatables.min.js", "pammsMoney.js" ]
    },
	{
      "matches": [ "https://my.privatefx.com/investor/details/*"],
      "js": [ "jquery-2.1.3.min.js", "invAcc.js" ]
    },
	{
      "matches": [ "https://my.privatefx.com/accounts/cash_out*"],
      "js": [ "jquery-2.1.3.min.js", "orders.js" ]
    }
	
  ]
}